<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <!--Use router 'admin' for admin route -->
    <router id="admin">
        <!--Define a custom route with id and frontName -->
        <route id="eatjoy_weathermodule" frontName="eatjoy_weathermodule">
            <!--The module which this route match to-->
            <module name="EatJoy_WeatherModule"/>
        </route>
    </router>
</config>
